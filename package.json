{
  "name": "simple-react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "build": "webpack --config ./config/webpack.prod.config.js",
    "start": "webpack-dev-server --config ./config/webpack.dev.config.js",
    "analyze": "webpack --config ./config/webpack.analyze.config.js",
    "fix:assets": "prettier --ignore-path .gitignore --write \"**/*.{json,md,scss}\"",
    "fix:code": "yarn lint:code -- --fix",
    "fix": "yarn fix:code && yarn fix:assets",
    "lint:code": "eslint --ignore-path .gitignore --ignore-path .prettierignore \"**/*.{js,jsx}\"",
    "lint": "yarn lint:code"
  },
  "lint-staged": {
    "*.{json,scss,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix --ignore-path .prettierignore",
      "git add"
    ]
  },
  "author": "Beaglefoot",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "dumper": "^1.0.12",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "2.1.0",
    "image-webpack-loader": "^4.3.1",
    "lint-staged": "8.1.5",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.9.2",
    "prettier": "1.17.0",
    "raw-loader": "^2.0.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.15.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.2",
    "redux": "^4.0.1"
  }
}
